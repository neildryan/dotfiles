[global_config]
  inactive_color_offset = 0.69
[keybindings]
  go_down = <Alt>j
  go_left = <Alt>h
  go_right = <Alt>l
  go_up = <Alt>k
  layout_launcher = None
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#1e2127"
    background_darkness = 0.58
    cursor_color = "#eeeeec"
    font = Monospace 13
    foreground_color = "#abb2bf"
    icon_bell = False
    palette = "#1e2127:#e06c75:#98c379:#d19a66:#61afef:#c678dd:#56b6c2:#abb2bf:#5c6370:#e06c75:#98c379:#d19a66:#61afef:#c678dd:#56b6c2:#ffffff"
    scrollback_lines = 3000
    scrollbar_position = hidden
    show_titlebar = False
    use_system_font = False
